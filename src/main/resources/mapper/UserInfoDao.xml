<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.testDemo.dao.UserInfoDao">

	<select id="infoList" resultType="com.example.testDemo.entity.UserInfoEntity">
		select * from user_info where USER_ACCONT = #{userAccont}
	
	</select>
	
	<select id="count" resultType="Integer">
		select dd from ad
	</select>
	
	<update id="buy" >
		UPDATE ad SET dd = #{dd}
	</update>



</mapper>
